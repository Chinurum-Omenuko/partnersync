	},
	"dependencies": {
		"@auth0/auth0-spa-js": "^2.1.3",
		"@clerk/clerk-js": "^5.34.3",
		"@supabase/ssr": "^0.5.2",
		"@supabase/supabase-js": "^2.46.1",
		"@sveltejs/adapter-node": "^5.2.9",
		"@sveltejs/kit": "^2.0.0",
		"clerk-sveltekit": "^0.4.2",
		"firebase": "^11.0.2",
		"svelte-clerk": "^0.4.5",
		"svelte-modals": "^2.0.0"
	}
}
